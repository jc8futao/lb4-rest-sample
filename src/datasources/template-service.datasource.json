{
  "name": "templateservice",
  "connector": "rest",
  "debug": "true",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    }
  },
  "operations": [    
    {
      "template": {
        "method": "POST",
        "url": "http://localhost:5003/",
        "body": "{body}"
      },
      "functions": {
        "gettemplate": ["body"]
      }
    }
  ]
}